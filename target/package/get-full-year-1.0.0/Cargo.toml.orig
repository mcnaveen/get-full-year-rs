[package]
name = "get-full-year"
version = "1.0.0"
edition = "2021"
authors = ["Naveen MC <me@mcnaveen.com>"]
description = "An unofficial Rust client for getfullyear.com that helps you get the full year."
license = "MIT OR Apache-2.0"
repository = "https://github.com/mcnaveen/get-full-year-rs"
documentation = "https://docs.rs/get-full-year"
readme = "README.md"
keywords = ["year", "fetching", "enterprise", "rust"]
categories = ["data-structures"]

[dependencies]
reqwest = { version = "0.11", features = ["json"] }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
tokio = { version = "1", features = ["full"] }